{
  "name": "StartPage",
  "description": "Start page",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["tabs", "bookmarks", "fontSettings", "favicon"],
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
